type Animal {
    id: Int!,
    name: String,
    breed: String
}

type ResponseAnimalDto{
    id: Int,
    name: String,
    breed: String,
    links: [String],
}

type Query {
    getAnimals: [Animal!]!
    getAnimal(id: Int!): ResponseAnimalDto!
}

type Mutation {
    createAnimal(name: String, breed: String): Animal
    addAnimalImage(id: Int, imageLinks: [String]): ResponseAnimalDto
}